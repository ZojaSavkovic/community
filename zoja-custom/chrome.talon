# activate this .talon file if the current app is stated
# you can find app names by running ui.apps() in the REPL
app.name: Google Chrome
-

# define some voice commands
left: key("ctrl-pgup")
right: key("ctrl-pgdown")
reopen: key("ctrl-shift-t")
load: key("ctrl-f5")
dev: key("ctrl-shift-i")

#gmail
not: key("shift-8 shift-u")
open: key("shift-i")
reset: key("shift-8 shift-n")
